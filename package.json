		"devextreme-react": "23.1.5",
		"exceljs": "^4.3.0",
		"file-saver": "^2.0.5",
		"i18next": "^23.4.9",
		"json-server-auth": "^2.1.0",
		"jspdf": "^2.5.1",
		"moment": "^2.29.4",
